{
  "name": "homepage",
  "version": "1.0.0",
  "description": "@coot homepage",
  "dependencies": {
    "bower": "^1.8.4",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.9.4",
    "gulp-handlebars": "^5.0.2",
    "gulp-htmlmin": "^4.0.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-nunjucks": "^4.0.0",
    "handlebars": "^4.0.11",
    "nunjucks": "^3.1.3",
    "workbox-cli": "^3.3.1"
  },
  "devDependencies": {
    "workbox-build": "^3.3.0",
    "workbox-precaching": "^3.3.1",
    "workbox-routing": "^3.3.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "buildHTML": "gulp",
    "hs:test": "doctest -fdiagnostics-color=never haskell/free-garden.hs",
    "haddock": "haddock --optghc=\"-no-link\" --optghc=\"-no-hs-main\" -o haskell/html -h --no-print-missing-docs --hyperlinked-source haskell/*.hs",
    "build": "npm run haddock; gulp",
    "start": "http-server -c0 dist",
    "postinstall": "bower install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coot/homepage.git"
  },
  "keywords": [
    "homepage",
    "pwa"
  ],
